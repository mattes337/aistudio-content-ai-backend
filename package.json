{
  "name": "aistudio-content-ai-backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target bun",
    "start": "bun dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "bun run scripts/runMigrations.ts",
    "db:seed": "bun run src/database/seed.ts",
    "migration:run": "bun run scripts/runMigrations.ts",
    "migration:create": "bun run scripts/createMigration.ts",
    "migration:status": "bun -e \"import('./src/utils/migrationRunner.js').then(m => m.runMigrationsCLI())\"",
    "migration:test": "bun run src/utils/testMigrations.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ai": "^6.0.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonrepair": "^3.13.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pgvector": "^0.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
}
